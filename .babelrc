{
  "presets": [
    "stage-0",
    [ "env", {
        "loose": true,
        "modules": false,
        "targets": {
          "browsers": [ "last 2 versions" ]
        }
      }
    ],
    "react"
  ],

  "plugins": [
    "react-hot-loader/babel",
    [ "transform-class-properties", { "spec": true } ]
  ]
}
